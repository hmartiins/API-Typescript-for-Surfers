{
  "name": "api-typescript-for-surfers",
  "version": "1.0.0",
  "description": "Typescript api for surfes nodejs",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./__tests__ --ext .ts",
    "lint:fix": "eslint ./src ./__tests__ --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./__tests__ --runInBand",
    "test:unit": "jest",
    "style:check": "prettier --check 'src/**/*.ts' '__tests__/**/*.ts'",
    "style:write": "prettier --fix 'src/**/*.ts' '__tests__/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hmartiins/API-Typescript-for-Surfers.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Henrique Martins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hmartiins/API-Typescript-for-Surfers/issues"
  },
  "homepage": "https://github.com/hmartiins/API-Typescript-for-Surfers#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.37",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.18",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.11",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "supertest": "^6.0.1"
  }
}
